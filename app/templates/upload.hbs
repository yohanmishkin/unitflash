<div class="demo-wrapper">
  {{x-file-input action="uploadManyFiles" class="custom-class"
    id="spec-file-input-three" multiple=true alt="Upload cards"}}

  {{#each flashcards as |flashcard|}}
    <p class="file-name">
      {{flashcard.name}}
      {{flashcard.front}}
      {{flashcard.back}}
    </p>
  {{/each}}
</div>