<div class="home-nav">
	{{link-to 'Unitflash' 'index' class='test-nav-home' tagName='h3'}}
</div>

<div class="deck">
	{{unitflash-card card=model front=true flipCard=(action "changeCard")}}
</div>

<div class="travel-links">
	{{#unless isFirstCard}}
		{{#link-to 'practice' previousId class='test-previous-card' disabled=isFirstCard}}Previous{{/link-to}}
	{{/unless}}
	{{#if isLastCard}}
		{{link-to 'Complete' 'complete' class='test-complete-practice complete-link'}}
	{{else}}
		{{#link-to 'practice' nextId class='test-next-card'}}Next{{/link-to}}
	{{/if}}
</div>
